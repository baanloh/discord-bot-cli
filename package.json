{
    "name": "discord-bot-cli",
    "version": "5.3.0",
    "description": "An easy way to build a command-based discord bot.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "dbc": "./dist/bin/cli.js"
    },
    "scripts": {
        "v": "npm --no-git-tag-version version",
        "test": "npm run lint && npm run test:build",
        "test:build": "tsc",
        "start": "ts-node test/index.ts",
        "start:log": "ts-node test/log.ts",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "prettier:format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "docs": "typedoc ./src/",
        "build": "tsc && npm run copy:assets",
        "build:clean": "shx rm -rf ./dist",
        "rebuild": "npm run build:clean && npm run build",
        "prepublishOnly": "npm run rebuild",
        "copy:assets": "shx cp -r ./src/assets ./dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tguichaoua/discord-bot-cli.git"
    },
    "keywords": [
        "discord",
        "bot",
        "cli"
    ],
    "author": "Tristan Guichaoua",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tguichaoua/discord-bot-cli/issues"
    },
    "homepage": "https://github.com/tguichaoua/discord-bot-cli",
    "devDependencies": {
        "@types/chalk": "^2.2.0",
        "@types/node": "^13.13.14",
        "@types/ws": "^7.2.6",
        "@typescript-eslint/eslint-plugin": "^3.6.0",
        "@typescript-eslint/parser": "^3.6.0",
        "discord.js": "^12.2.0",
        "eslint": "^7.4.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "prettier": "^2.0.5",
        "shx": "^0.3.2",
        "ts-node": "^8.10.2",
        "typedoc": "^0.17.7",
        "typescript": "^3.9.3"
    },
    "dependencies": {
        "chalk": "^4.1.0",
        "meow": "^6.1.1"
    },
    "peerDependencies": {
        "discord.js": "^12.2.0"
    }
}