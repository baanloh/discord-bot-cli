{
    "name": "discord-bot-cli",
    "version": "5.1.0",
    "description": "An easy way to build a command-based discord bot.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "dbc": "./bin/cli.js"
    },
    "scripts": {
        "v": "npm --no-git-tag-version version",
        "docs": "typedoc ./src/",
        "build": "tsc",
        "clean": "rm -rf ./dist",
        "rebuild": "npm run clean && npm run build",
        "prepublishOnly": "npm run rebuild"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/baanloh/discord-bot-cli.git"
    },
    "keywords": [
        "discord",
        "bot",
        "cli"
    ],
    "author": "Tristan Guichaoua",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/baanloh/discord-bot-cli/issues"
    },
    "homepage": "https://github.com/baanloh/discord-bot-cli#readme",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^3.6.0",
        "@typescript-eslint/parser": "^3.6.0",
        "discord.js": "^12.2.0",
        "eslint": "^7.4.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "prettier": "^2.0.5",
        "typedoc": "^0.17.7",
        "typescript": "^3.9.3"
    },
    "dependencies": {
        "@types/node": "^13.13.9",
        "@types/ws": "^7.2.5",
        "meow": "^6.1.1"
    },
    "peerDependencies": {
        "discord.js": "^12.2.0"
    }
}
